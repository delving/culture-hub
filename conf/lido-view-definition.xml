                                <field path="lido:event/lido:eventActor/lido:actorInRole/lido:actor/lido:genderActor" label="_hub.lido.genderActor"/>
                                <field path="lido:event/lido:eventActor/lido:actorInRole/lido:actor/lido:roleActor/lido:term" label="_hub.lido.roleActor"/>
                                <!--<field path="lido:event/lido:eventActor/lido:roleActor/lido:term" label="_hub.lido.eventRoleActor"/>-->
