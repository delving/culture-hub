<?xml version ="1.0" encoding="UTF-8"?>

<views>
    <view name="html">
        <row>
            <section id="general" >
                <field path="/aff:record/aff:thing/aff:displayTitle" label="thing.title" />
                <field path="/aff:record/aff:thing/aff:displayDescription" label="thing.description"/>
            </section>
        </row>
        <row>
            <section id="events">
                <list path="/aff:record/aff:events/aff:event">
                    <field label="thing.title" path="aff:displayEvent"/>
                    <field label="thing.date" path="aff:displayDate"/>
                </list>
            </section>
        </row>
    </view>
    <view name="full">
        <elem name="record">
            <attrs>
                <attr prefix="xmlns" name="aff" value="http://schemas.delving.eu/aff/"/>
            </attrs>
            <elem name="admin">
                <elem name="displayDataProvider" expr="/aff:record/aff:admin/aff:displayDataProvider" />
                <elem name="displayDataProviderCountry" expr="/aff:record/aff:admin/aff:displayDataProviderCountry" />
                <elem name="displayProvider" expr="/aff:record/aff:admin/aff:displayProvider" />
                <elem name="defaultLang" expr="/aff:record/aff:admin/aff:defaultLang" />
            </elem>
            <elem name="thing">
                <elem name="uri" expr="/aff:record/aff:thing/aff:uri" />
                <elem name="category" expr="/aff:record/aff:thing/aff:category" />
                <elem name="displayIdentification" expr="/aff:record/aff:thing/aff:displayIdentification" />
                <elem name="displayTitle" expr="/aff:record/aff:thing/aff:displayTitle" />
                <elem name="displayDescription" expr="/aff:record/aff:thing/aff:displayDescription" />
                <list name="dimensions" path="/aff:record/aff:thing/aff:dimensions/aff:displayDimension">
                    <elem name="displayDimension" expr="." />
                </list>
                <list name="years" path="/aff:record/aff:thing/aff:years/aff:year">
                    <elem name="year" expr="."/>
                </list>
                <elem name="displayRights" expr="/aff:record/aff:thing/aff:displayRights" />
            </elem>
        </elem>
    </view>
</views>

