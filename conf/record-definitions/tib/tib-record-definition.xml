            <!--<opt key="7">Film/Video/DVD</opt>-->
            <!--<opt key="8">Anders:</opt>-->
        <!--</option-list>-->
    </options>

    <searchFields>
        <searchField name="THUMBNAIL" path="/record/tib:thumbLarge"/>
        <searchField name="SNIPPET" path="/record/delving:fullText"/>
        <searchField name="CREATOR" path="/record/dc:creator"/>
        <searchField name="SNIPPET" path="/record/dc:description"/>
        <searchField name="TITLE" path="/record/dc:title"/>
        <searchField name="OWNER" path="/record/europeana:dataProvider"/>
        <searchField name="LANDING_PAGE" path="/record/europeana:isShownAt"/>
    </searchFields>

